<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActionBarOverlay">false</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!--Botones transparentes-->
    <style name="botones_inicio">
        <item name="android:background">@drawable/botones_pantalla_inicio </item>
        <item name="android:textColor">@color/blanco</item>
    </style>

    <!--Boton Facebook-->
    <style name="boton_facebook">
        <item name="android:background">@drawable/boton_facebook </item>
        <item name="android:textColor">@color/blanco</item>

    </style>

    <!--Boton Perfil-->
    <style name="boton_perfil">
        <item name="android:background">@drawable/botones_perfil </item>
        <item name="android:textColor">@color/negro</item>
    </style>

    <!-- Boton Publicar proyecto-->
    <style name="boton_publicar">
        <item name="android:background">@drawable/boton_publicar_proyecto</item>
        <item name="android:textColor">@color/blanco</item>
    </style>
</resources>
